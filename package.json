{
  "name": "RN_APP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "clean:build": "cd android/ && ./gradlew clean &&  ./gradlew assembleRelease",
    "clean-android-bundle": "rm -rf ./bundles/android/*",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "clean-ios-bundle": "rm -rf ./bundles/ios/*",
    "push-android-staging": "code-push release-react RN_APP-Android android  --t 1.0.0 --dev false --d Staging --des '调整code-push更新' --m false",
    "deploy-history-android-staging": "code-push deployment history RN_APP-Android Staging",
    "deploy-history-android": "code-push deployment history RN_APP-Android Production",
    "push-android": "code-push release-react RN_APP-Android android  --t 1.0.0 --dev false --d Production --des '调整code-push更新' --m false",
    "publish:android": "export ENVFILE=.env.production && npx upload-pgy --apk=v7a --no-ios"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/viewpager": "^4.1.3",
    "@react-navigation/bottom-tabs": "^5.7.2",
    "@react-navigation/drawer": "^5.8.6",
    "@react-navigation/material-bottom-tabs": "^5.2.14",
    "@react-navigation/material-top-tabs": "^5.2.14",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "axios": "^0.19.2",
    "dva-core": "^2.0.2",
    "dva-loading": "^3.0.20",
    "fbemitter": "^2.1.1",
    "lottie-react-native": "3.4.0",
    "react": "16.13.1",
    "react-native": "0.63.1",
    "react-native-animatable": "^1.3.3",
    "react-native-code-push": "^6.3.0",
    "react-native-config": "^1.3.1",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-linear-animated-gradient-transition": "^0.1.6",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-paper": "^4.0.1",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-slider-x": "^0.11.3",
    "react-native-snap-carousel": "^4.0.0-beta.5",
    "react-native-sound": "^0.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.0",
    "react-native-tab-view-viewpager-adapter": "^1.1.0",
    "react-native-vector-icons": "^7.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-iconfont-cli": "^2.1.0",
    "react-native-upload": "^2.1.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
